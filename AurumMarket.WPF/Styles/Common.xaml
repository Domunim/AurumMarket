<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!--Colors-->
    <Color x:Key="ColorPrimary1">#22303C</Color>
    <Color x:Key="ColorPrimary2">#192734</Color>
    <Color x:Key="ColorBackground">#15202B</Color>
    <Color x:Key="ColorText1">#E4E6EB</Color>
    <Color x:Key="ColorText2">#8899A6</Color>


    <!--Brushes-->
    <SolidColorBrush x:Key="BrushPrimary1" Color="{StaticResource ColorPrimary1}"/>
    <SolidColorBrush x:Key="BrushPrimary2" Color="{StaticResource ColorPrimary2}"/>
    <SolidColorBrush x:Key="BrushBackground" Color="{StaticResource ColorBackground}"/>
    <SolidColorBrush x:Key="BrushText1" Color="{StaticResource ColorText1}"/>
    <SolidColorBrush x:Key="BrushText2" Color="{StaticResource ColorText2}"/>



    <!-- Dark Mode Colours
    
    [the higher, the darker]
    
    Background 15202B
    Card 192734
    Hover 22303C
    primary text E4E6EB (almost white)
    secondary text 8899A6 -->


</ResourceDictionary>